
<Button>:
    font_size:40
    size_hint:.2, .1
    
<FloatLayoutWidget>:
    canvas:
        Color:
            rgba:[1,1,1,1]
        Rectangle:
            size:self.size
            pos:self.pos

    Button:
        id:'l0'
        text:'C'
        background_color:.1,.5,.6,1
        pos:10,10
    
    Button:
        id:'l1'
        text:'Java'
        background_color:.1,.5,.6,1
        pos:100,100
    
    Button:
        id:'l2'
        text:'Python'
        background_color:.1,.5,.6,1
        pos:200,200
    
    Button:
        id:'l3'
        text:'Javascript'
        background_color:.1,.5,.6,1
        pos:300,300
