#:kivy 1.4
<RegisterWidget>:
	cols:2
	padding:20
	spacing:20
	Label:
		text:'Username'
	TextInput:
		password:False
		multiline:False
	Label:
		text:'Password'
	TextInput:
		password:True
	Label:
		text:'Gender'
	BoxLayout:
		orientation:'horizontal'
		CheckBox:
			group:'gender'
			active:True
		Label:
			text:'Male'
		CheckBox:
			group:'gender'
		Label:
			text:'Female'
	Label:
		text:'Favorate'
	BoxLayout:
		orientation:'vertical'
		BoxLayout:
			orientation:'horizontal'
			CheckBox:
			Label:
				text:'C'
		BoxLayout:
			orientation:'horizontal'
			CheckBox:
			Label:
				text:'C++'
		BoxLayout:
			orientation:'horizontal'
			CheckBox:
			Label:
				text:'Java'
		BoxLayout:
			orientation:'horizontal'
			CheckBox:
			Label:
				text:'Javascript'
		BoxLayout:
			orientation:'horizontal'
			CheckBox:
				active:True
			Label:
				text:'Python'
	Label:
		text:'College'
	Spinner:
		text:'C'
		values:('C', 'Java', 'Python')
	Label:
		text:'infomation'
	TextInput:
		multiline:True
	Button:
		text:'OK'
	Button:
		text:'Cancel'